#include<bits/stdc++.h>
using namespace std;
void bubblesort(int a[],int size)
{
int temp,ctr=0;
for(int i=0;i<size;i++)
{
 for(int j=0;j<=(size-1)-i;j++)
 if(a[j]>a[j+1])
 {
  temp=a[j];
  a[j]=a[j+1];
  a[j+1]=temp;
 }}
 cout<<"array after iteration"<<++ctr<<"\n";
 for(int k=0;k<size;k++)
 {cout<<a[k]<<" ";}
 cout<<endl;
 }
 int main()
 {
 int n;
 cout<<"Enter size of array \n";
 cin>>n;
 int a[n];
 cout<<"Enter the elements \n";
 for(int i=0;i<n;i++)
 cin>>a[i];
 bubblesort(a,n);
 return 0;
 }
